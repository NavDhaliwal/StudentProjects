<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="info.student"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        
        
        <activity android:name="Start">
                    <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
                
            </intent-filter>
        
            <intent-filter>
                <action android:name="submit.back"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
            
            <intent-filter>
                <action android:name="back.start"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
         <activity android:name="Delete_multiple">
            <intent-filter>
                <action android:name="delete_multiple"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
            </activity>
        <activity android:name="Add">
            <intent-filter>
                <action android:name="add"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
            </activity>
             <activity android:name="Take_pic">
            <intent-filter>
                <action android:name="take_pic"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
            </activity>
            
        
        <activity android:name="Edit">
        <intent-filter>
                <action android:name="edit.start"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
        </intent-filter>
        </activity>
        <activity android:name="Show">
        <intent-filter>
                <action android:name="show"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
        </intent-filter>
        </activity>
        <activity android:name="Show1">
        <intent-filter>
                <action android:name="show1"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
        </intent-filter>
        </activity>
          <uses-permission android:name="android.permission.CAMERA" />
 <uses-feature android:name="android.hardware.camera" />
 <uses-feature android:name="android.hardware.camera.autofocus" />
        
        </application>
        
</manifest>